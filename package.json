{
  "name": "@slimio/micro",
  "version": "0.1.0",
  "description": "NodeJS C NAPI low level binding to get high resolution timestamp (in microseconds)",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r",
    "coverage": "nyc ava",
    "report": "nyc report --reporter=html",
    "install": "node-gyp rebuild"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Micro.git"
  },
  "keywords": [],
  "author": "SlimIO",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/SlimIO/Micro/issues"
  },
  "homepage": "https://github.com/SlimIO/Micro#readme",
  "dependencies": {
    "bindings": "^1.3.0",
    "node-addon-api": "^1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^2.0.3",
    "@slimio/is": "^1.4.0",
    "@types/node": "^10.12.11",
    "ava": "^0.25.0",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "jsdoc": "^3.5.5",
    "node-gyp": "^3.8.0",
    "nyc": "^13.1.0"
  }
}
